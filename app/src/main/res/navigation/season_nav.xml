<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/season_nav"
    app:startDestination="@id/seasonDress">

    <fragment
        android:id="@+id/seasonDress"
        android:name="com.darotapp.meetupapp.ui.SeasonDress"
        android:label="fragment_season_dress"
        tools:layout="@layout/fragment_season_dress" >
        <action
            android:id="@+id/toMainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/toSignin"
            app:destination="@id/signInFragment2" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.darotapp.meetupapp.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/signInFragment2"
        android:name="com.darotapp.meetupapp.ui.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <argument
            android:name="user"
            app:argType="com.darotapp.meetupapp.model.UserData"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="goOut"
            app:argType="boolean"
            android:defaultValue="false" />
    </fragment>
</navigation>